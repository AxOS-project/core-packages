pkgname="axskel"
pkgver="1.6.2"
pkgrel="1"
pkgdesc="skel configs and looks for AxOS"
arch=("x86_64")


package(){
   mkdir -p ${pkgdir}/etc/skel/
   mkdir -p ${pkgdir}/usr/share/
   cp -r ${srcdir}/.config/ ${pkgdir}/etc/skel/
   cp -r ${srcdir}/.local/ ${pkgdir}/etc/skel/
   cp -r ${srcdir}/plasma/ ${pkgdir}/usr/share/
}
